			<script src="https://cdnjs.cloudflare.com/ajax/libs/lodash.js/4.17.11/lodash.min.js"></script>
      <script src="https://cdnjs.cloudflare.com/ajax/libs/hammer.js/2.0.8/hammer.js"></script>

      <script type="text/javascript">
        var map;

        /* ================================= */
        /* ============ Filters ============ */
        /* ================================= */
        /* Gesture Behavior */
        const formFilters = document.querySelector("#form-filters");
        const hammerFilters = new Hammer(formFilters);

        hammerFilters.on('tap', function(e) {
          const value = e.target.getAttribute('data-value');

          toggleFilters(value);
          resetFilters(markers);

          filterMap(value);
        });

         /* Sorting */
         var markers = new Array();
         var searchMarkers = new Array();
         var infoWindows = new Array();
				 var filters = new Array();

				 function filterMap(value) {
					 if(filters.length) {
						 _.forEach(markers, function(m) {
							 const isSelected = _.find(filters, function(f) { return m.icon.fillColor === f });
							 if(!isSelected) {
								 m.setVisible(false);
							 }
						 });
					 }
				 }

				 function resetFilters(markers) {
					 _.forEach(markers, function(m) {
							m.setVisible(true);
					 })
				 }

         /* Menu */
				 function toggleFilters(value) {
					 const filter = _.find(filters, function(f) { return f === value });

					 if(filter) {
						 _.pull(filters, value);
					 } else {
						 filters.push(value);
					 }
				 }

         function toggleMenu() {
           document.querySelector('.filters').classList.toggle('isClosed');
           if(document.querySelector('.filters').classList.contains('isClosed')) {
             map.panBy(155, 0);
           } else {
             map.panBy(-155, 0);
           }
         }

         /* ================================= */
         /* ======== Search Results ========= */
         /* ================================= */
         /* Gesture Behavior */
         const resultsList = document.querySelector("#results");
         const hammerResults = new Hammer(resultsList);

         hammerResults.on('tap', function(e) {
           const lat = e.target.getAttribute('data-lat');
           const lng = e.target.getAttribute('data-lng');

           map.setCenter(new google.maps.LatLng(lat,lng));
           map.panBy(-150, 0);

           const infoWindow = _.find(infoWindows, function(iw) {
             return iw._marker.getPosition().lat() === parseFloat(lat) && iw._marker.getPosition().lng() === parseFloat(lng);
           });

           infoWindow.open();
         });
		  </script>

